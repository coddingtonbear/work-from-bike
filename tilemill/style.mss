Map {
  background-color: #202020;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #fff;
}

#roads {
  line-width:1;
  line-color:#168;
  line-opacity:0.25;
}


#natural {
  line-color:#594;
  line-width:0.5;
  polygon-opacity:0.5;
  polygon-fill:#ae8;
}